<?xml version='1.0' encoding='utf-8'?>
<widget id="org.bcsphere.bcautotest" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <name>BCAutoTest</name>
    <description>
        Bluetooth LE test tool.
    </description>
    <author email="xudong.chen@jumacc.com" href="http://www.bcsphere.org">
        JUMA Technology
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="BCBluetooth">
        <param name="android-package" value="org.bcsphere.bluetooth.BCBluetooth" />
    </feature>
</widget>
